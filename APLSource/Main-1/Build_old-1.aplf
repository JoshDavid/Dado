 Build_old←{
     ⍝ ⍺ ←→ Project Folder
     ⍝ ⍵ ←→ Version
     ⍺←0
     v←⍵
     pf←GetProjectFolder ⍺
     cl←#.GetCommandLineArgs
     e←0⊃cl
     q←{'"',⍵,'"'}
     c←##.AcreConfig
     ws←c.ProjectFolder,'Assets/Runtime/Build.dws'
     a←q ws
     a,←' ProjectFolder=',q pf
     a,←' Version=',q v
     a,←' maxws=2g'
     a,←' Trace=',⍕⍺
     r←RunProcess e a
     r.ExitCode(↑⌽r.StdErr)
 }
