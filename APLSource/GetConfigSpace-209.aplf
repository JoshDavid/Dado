 GetConfigSpace←{
     ⍝ ⍵ ←→ ProjectFolder
     f←GetProjectFolder ⍵
     c←1 ⎕FIX'file://',f,'acre.config'
     p←⌽(~f∊'/\')⊆f
     c.ProjectFolder←f
     c.ProjectName←0⊃p
     c.ProjectAuthor←1⊃p
     c.FullProjectName←c.ProjectAuthor,'/',c.ProjectName
     c.PackageFolder←f,'dist/Package/',c.ProjectName,'/'
     c.ApplicationFolder←f,'dist/Application/',c.ProjectName,'/'
     c
 }
